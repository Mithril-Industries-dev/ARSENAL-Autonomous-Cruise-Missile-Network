<svg xmlns="http://www.w3.org/2000/svg" width="64" height="64" viewBox="0 0 64 64">
  <!-- Background circle -->
  <circle cx="32" cy="32" r="28" fill="#2a2a2a"/>
  
  <!-- Gear teeth -->
  <g fill="#4a9eff">
    <rect x="29" y="6" width="6" height="10"/>
    <rect x="29" y="48" width="6" height="10"/>
    <rect x="6" y="29" width="10" height="6"/>
    <rect x="48" y="29" width="10" height="6"/>
    
    <!-- Diagonal teeth -->
    <rect x="11" y="11" width="6" height="10" transform="rotate(45 14 16)"/>
    <rect x="47" y="11" width="6" height="10" transform="rotate(-45 50 16)"/>
    <rect x="11" y="43" width="6" height="10" transform="rotate(-45 14 48)"/>
    <rect x="47" y="43" width="6" height="10" transform="rotate(45 50 48)"/>
  </g>
  
  <!-- Gear center -->
  <circle cx="32" cy="32" r="16" fill="#4a9eff"/>
  
  <!-- Gear hole -->
  <circle cx="32" cy="32" r="8" fill="#2a2a2a"/>
